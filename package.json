{
  "name": "postcss-reference",
  "version": "1.0.0",
  "description": "PostCSS plugin for importing CSS files and partials.  Will not output rules unless directly rng' stylesheet; a la LESS's `@import (reference) 'filename'`.  See http://lesscss.org/features/#import-optnce",
  "scripts": {
    "prepublish": "babel test/test.js --out-dir dist --ignore /test/",
    "test-unformatted": "babel-tape-runner \"dist/test/*.js\"",
    "test": "npm run test-unformatted | tap-spec"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "reference",
    "import",
    "extend"
  ],
  "author": {
    "name": "Samuel Allen",
    "email": "dehuszar@gmail.com"
  },
  "license": "MIT",
  "repository": "dehuszar/postcss-reference",
  "bugs": {
    "url": "https://github.com/dehuszar/postcss-reference/issues"
  },
  "homepage": "https://github.com/dehuszar/postcss-reference",
  "dependencies": {
    "array-find": "^1.0.0",
    "postcss": "^5.0.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-tape-runner": "^1.2.0",
    "postcss-import": "^7.0.0",
    "postcss-nested": "^1.0.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1"
  }
}
